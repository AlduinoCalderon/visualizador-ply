{
  "rewrites": [
    { "source": "/embedded", "destination": "/src/embedded.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM *"
        }
      ]
    }
  ],
  "outputDirectory": "dist"
} 